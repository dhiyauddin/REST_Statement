<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">

<hibernate-mapping>
	<class name="com.example.table.Financial_Detail"
		table="financial1000">
		<id name="fn_detail_id">
			<generator class="assigned"></generator>
		</id>
		<property name="fn_trans_date"></property>
		<property name="fn_date"></property>
		<property name="fn_detail"></property>
		<property name="fn_myr"></property>
		<property name="fn_trans_type"></property>
		<property name="create_by"></property>

	</class>

</hibernate-mapping>  